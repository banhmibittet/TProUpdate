<html>
<head>
    <meta charset="utf-8">
    <title>RockCaptcha Funcaptcha Extension</title>
    <meta name="viewport" content="initial-scale=1, maximum-scale=1"/>
    <meta name="description" content="Chrome Extension Template"/>
    <script src="config.js"></script>
    <script src="popup.js" type="text/javascript"></script>
    <link href="popup.css" rel="stylesheet"/>
</head>
<body>
<div id="layout">
    <div id="main">
        <div class="content">
            <form class="pure-form pure-form-stacked" disabled="true">
                <fieldset>
                    <legend id="title">
                        <a href="https://rockcaptcha.com">Rockcaptcha</a> <span id="balance">
                    </legend>
                    <label for="api-key"></label><input type="email" id="api-key" placeholder="API Key"/>
                    <label>
                        <span id="status"></span>
                    </label>
                    <button id="save-btn" type="submit" class="pure-button pure-button-primary">Save</button>
                </fieldset>
            </form>
        </div>
    </div>
</div>
</body>
</html>
